{
    "name": "symkit/faq-bundle",
    "description": "A flexible and modular FAQ (Frequently Asked Questions) management bundle for Symfony applications.",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "doctrine/doctrine-bundle": "^2.11 || ^3.0",
        "doctrine/orm": "^3.6",
        "symfony/framework-bundle": "^7.0 || ^8.0",
        "symfony/validator": "^7.0 || ^8.0",
        "symkit/crud-bundle": "~0.0.1",
        "symkit/form-bundle": "~0.0.1"
    },
    "autoload": {
        "psr-4": {
            "Symkit\\FaqBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symkit\\FaqBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symkit/bundle-ai-kit": "~0.0.1",
        "deptrac/deptrac": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "infection/infection": "^0.29",
        "nyholm/symfony-bundle-test": "^3.0",
        "phpro/grumphp": "^2.0",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.0"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "phpro/grumphp": true,
            "symkit/bundle-ai-kit": true
        }
    },
    "extra": {
        "bundle-ai-kit": {
            "editors": ["cursor"],
            "skills": [
                "symfony-bundle-core",
                "symfony-bundle-ux",
                "symfony-bundle-doctrine",
                "symfony-bundle-flex",
                "symfony-bundle-quality"
            ]
        }
    }
}
